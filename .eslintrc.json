{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "env": {
    "es2022": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": 0,
    "max-len": ["error", { "code": 140, "tabWidth": 4 }]
  }
}